<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.scxxwb.net.admin.modules.sys.dao.TWbAreaDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.scxxwb.net.admin.modules.sys.entity.TWbAreaEntity" id="tWbAreaMap">
        <result property="areaCode" column="area_code"/>
        <result property="parentId" column="parent_id"/>
        <result property="name" column="name"/>
        <result property="sort" column="sort"/>
        <result property="id" column="id"/>
    </resultMap>

    <select id="selectByAreaCode" parameterType="Integer" resultType="TWbAreaEntity">
        select * from t_wb_area where area_code=#{areaCode}
    </select>

</mapper>